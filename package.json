{
  "name": "mobu-elm",
  "private": true,
  "version": "0.0.1",
  "description": "mobu timer",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "serve dist -l 8938 -n",
    "test": "elm-test",
    "lint": "elm-review && npm run eslint",
    "check": "npm run format:check && npm test && npm run lint",
    "preview": "vite preview",
    "format:check": "dprint check && npx elm-format --validate src",
    "format:fix": "dprint fmt && npx elm-format src",
    "eslint": "eslint --ignore-pattern \"src/**/*d.ts\" \"src/**/*.{js,ts,tsx}\"",
    "eslint:fix": "eslint --ignore-pattern \"src/**/*d.ts\" --fix \"src/**/*.{js,ts,tsx}\""
  },
  "author": "Kenichi Kamiya <kachick1@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "dprint": "^0.34.1",
    "elm-format": "^0.8.5",
    "elm-json": "^0.2.13",
    "elm-review": "^2.9.1",
    "elm-test": "^0.19.1-revision10",
    "eslint": "^8.30.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "vite": "^3.2.5",
    "vite-plugin-elm": "^2.7.2"
  }
}
