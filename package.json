{
  "name": "emobu",
  "private": true,
  "version": "0.0.1",
  "description": "mobu timer",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "serve dist -l 8938 -n",
    "test": "elm-test",
    "lint": "elm-review && npm run eslint && npm run stylelint:check",
    "check": "npm run format:check && npm run typecheck && npm test && npm run lint",
    "preview": "vite preview",
    "format:check": "dprint check && npx elm-format --validate src",
    "format:fix": "dprint fmt && npx elm-format src",
    "eslint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "stylelint:check": "npx stylelint '{src,public}/**/*.css'",
    "stylelint:fix": "npx stylelint '{src,public}/**/*.css' --fix",
    "typecheck": "tsc"
  },
  "author": "Kenichi Kamiya <kachick1@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "elm-format": "^0.8.7",
    "elm-json": "^0.2.13",
    "elm-review": "^2.10.2",
    "elm-test": "^0.19.1-revision12",
    "eslint": "^8.40.0",
    "stylelint": "^15.6.1",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.6",
    "vite-plugin-elm": "^2.8.0"
  },
  "dependencies": {
    "elm": "^0.19.1-5"
  }
}
